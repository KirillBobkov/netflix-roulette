{
  "name": "netflix-roulette",
  "version": "1.0.0",
  "description": "The idea is to create a single page application, which will allow users to\r search the Movies DB database.",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=development  node ./server",
    "prod": "cross-env NODE_ENV=production  node ./server",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.client.js",
    "build": "npm run build:client && npm run build:server",
    "test": "jest --coverage --updateSnapshot",
    "test:watch": "npm test -- --watch",
    "cypress": "cypress open",
    "flow": "flow",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore --fix"
  },
